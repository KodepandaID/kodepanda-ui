{
  "name": "@zenbu-ui/react",
  "version": "0.1.15",
  "description": "Zenbu-UI - React UI components with easy customization, don't worry about CSS anymore.",
  "author": "Yudha Pratama <yudha.webdev@gmail.com>",
  "homepage": "https://github.com/KodepandaID/zenbu-ui#readme",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KodepandaID/zenbu-ui.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/KodepandaID/zenbu-ui/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "concurrently yarn:build:*",
    "lint": "concurrently yarn:lint:*",
    "version": "yarn build",
    "build:esm": "cross-env BABEL_ENV=esm babel src --root-mode upward --extensions .js,.jsx -d dist/esm --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .js,.jsx -d dist/cjs --source-maps",
    "lint:src": "eslint src --ext .js,.jsx --config ../../.eslintrc.json"
  },
  "dependencies": {
    "@zenbu-ui/accordion": "0.1.9",
    "@zenbu-ui/avatar": "0.1.9",
    "@zenbu-ui/badge": "0.1.9",
    "@zenbu-ui/box": "0.1.12",
    "@zenbu-ui/breadcrumb": "0.1.9",
    "@zenbu-ui/button": "0.1.12",
    "@zenbu-ui/card": "0.1.12",
    "@zenbu-ui/collapse": "0.1.9",
    "@zenbu-ui/container": "0.1.10",
    "@zenbu-ui/divider": "0.1.10",
    "@zenbu-ui/dropdown": "0.1.12",
    "@zenbu-ui/empty-state": "0.1.9",
    "@zenbu-ui/grid": "0.1.9",
    "@zenbu-ui/icon": "0.1.9",
    "@zenbu-ui/image": "0.1.13",
    "@zenbu-ui/input": "0.1.12",
    "@zenbu-ui/list": "0.1.10",
    "@zenbu-ui/loader": "0.1.9",
    "@zenbu-ui/menu": "0.1.13",
    "@zenbu-ui/message": "0.1.9",
    "@zenbu-ui/modal": "0.1.13",
    "@zenbu-ui/notification": "0.1.9",
    "@zenbu-ui/pagination": "0.1.12",
    "@zenbu-ui/popover": "0.1.13",
    "@zenbu-ui/progress": "0.1.9",
    "@zenbu-ui/rating": "0.1.9",
    "@zenbu-ui/skeleton": "0.1.9",
    "@zenbu-ui/statistic": "0.1.9",
    "@zenbu-ui/switch": "0.1.9",
    "@zenbu-ui/table": "0.1.11",
    "@zenbu-ui/tabs": "0.1.9",
    "@zenbu-ui/tags": "0.1.9",
    "@zenbu-ui/themes": "0.1.5",
    "@zenbu-ui/tooltip": "0.1.10",
    "@zenbu-ui/typography": "0.1.10",
    "@zenbu-ui/upload": "0.1.12"
  }
}
